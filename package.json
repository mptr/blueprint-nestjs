{
	"name": "blueprint-nestjs",
	"version": "0.0.0",
	"description": "",
	"author": "",
	"private": true,
	"license": "UNLICENSED",
	"scripts": {
		"prepare": "if test \"$CI\" != 1; then husky install ; fi",
		"==> RELEASE": "",
		"prebuild": "rimraf dist",
		"build": "nest build",
		"==> LANUCH": "",
		"start": "nest start",
		"start:dev": "nest start --watch",
		"start:prod": "node dist/main",
		"==> LINT": "",
		"prettier": "prettier --write \"{src,cypress}/**/*.{js,json,css,scss,less,md,ts,html,component.html}\"",
		"prettier:check": "prettier --check \"{src,cypress}/**/*.{js,json,css,scss,less,md,ts,html,component.html}\"",
		"lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
		"==> TESTING": "",
		"test:debug": "node --inspect-brk\n  -r tsconfig-paths/register\n  -r ts-node/register node_modules/.bin/jest\n  --runInBand",
		"test:unit": "jest --config ./jest.config.ts",
		"test:unit-ci": "$npm_execpath run test:unit --ci --forceExit --coverage",
		"test:e2e": "jest --config ./test/jest-e2e.config.ts",
		"test:e2e-ci": "$npm_execpath run test:e2e --ci --forceExit --coverage",
		"test:all-coverage": "test/merge-coverage.sh",
		"==> DEVENV": "",
		"devenv:setup": "docker compose -f dev/docker-compose.yml --profile dev up -d",
		"devenv:setup-ci": "docker compose -f dev/docker-compose.yml --profile ci up -d",
		"devenv:teardown": "docker compose -f dev/docker-compose.yml down"
	},
	"engines": {
		"node": ">=16.19.0",
		"pnpm": ">=8.1.0"
	},
	"dependencies": {
		"@nestjs/common": "9.4.0",
		"@nestjs/config": "2.3.1",
		"@nestjs/core": "9.4.0",
		"@nestjs/mapped-types": "1.2.2",
		"@nestjs/platform-express": "9.4.0",
		"class-transformer": "0.5.1",
		"class-validator": "0.14.0",
		"reflect-metadata": "0.1.13",
		"rxjs": "7.8.0"
	},
	"devDependencies": {
		"@commitlint/cli": "17.5.1",
		"@commitlint/config-conventional": "17.4.4",
		"@nestjs/cli": "9.3.0",
		"@nestjs/schematics": "9.1.0",
		"@nestjs/testing": "9.4.0",
		"@semantic-release/commit-analyzer": "9.0.2",
		"@types/express": "4.17.17",
		"@types/jest": "29.5.0",
		"@types/node": "18.15.11",
		"@types/supertest": "2.0.12",
		"@typescript-eslint/eslint-plugin": "5.57.1",
		"@typescript-eslint/parser": "5.57.1",
		"commitlint": "17.5.1",
		"conventional-changelog-conventionalcommits": "5.0.0",
		"eslint": "8.37.0",
		"eslint-config-prettier": "8.8.0",
		"eslint-plugin-import": "2.27.5",
		"eslint-plugin-prettier": "4.2.1",
		"husky": "8.0.3",
		"jest": "29.5.0",
		"lint-staged": "13.2.0",
		"nyc": "^15.1.0",
		"prettier": "2.8.7",
		"semantic-release": "21.0.1",
		"source-map-support": "0.5.21",
		"supertest": "6.3.3",
		"ts-jest": "29.1.0",
		"ts-loader": "9.4.2",
		"ts-node": "10.9.1",
		"tsconfig-paths": "4.2.0",
		"tslint-config-prettier": "1.18.0",
		"typescript": "5.0.3"
	}
}
